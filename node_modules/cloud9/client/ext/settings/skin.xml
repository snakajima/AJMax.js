<?xml version='1.0'?>
<a:skin xmlns:a="http://ajax.org/2005/aml" xmlns="http://www.w3.org/1999/xhtml">
    <a:bar name="bar-preferences">
        <a:style><![CDATA[
            .bar-preferences {
                position         : relative;
                overflow-y       : hidden;
                overflow-x       : hidden;
                /*background-image : url(images/panelbg.png);*/
                color            : #202020;
                font-family      : Tahoma, Arial;
                font-size        : 12px;
                text-shadow : rgba(255, 255, 255, 0.8) 0px 1px 0px;
            }
            
            .bar-preferences .header {
                background-color : #f6f6f6;
                height           : 39px;
                padding          : 0;
                cursor           : default;
                border-bottom    : 1px solid white;
                font-weight      : bold;
                font-size        : 13px;
                position         : relative;
                overflow         : visible;
                display          : block;
                font-family      : Tahoma, Arial;
                font-size        : 13px;
                color            : #303030;
                font-weight      : bold;
                margin           : 5px 4px 10px 4px;
                
                text-shadow : none;
                
                -moz-border-radius    : 3px;
                -webkit-border-radius : 3px;
                
                -moz-box-shadow: 1px 1px 1px -1px rgba(0, 0, 0, 0.15);
                -webkit-box-shadow: 1px 1px 1px -1px rgba(0, 0, 0, 0.15);
                box-shadow: 1px 1px 1px -1px rgba(0, 0, 0, 0.15);
                
                filter: progid:DXImageTransform.Microsoft.gradient(GradientType=0,startColorstr='#ffffff', endColorstr='#ebebeb');
                -ms-filter: "progid:DXImageTransform.Microsoft.gradient(GradientType=0,startColorstr='#ffffff', endColorstr='#ebebeb')";
                
                background: -webkit-gradient(linear, left top, left bottom, from(rgba(255, 255, 255, 1)), color-stop(1, rgba(235, 235, 235,1)));
                -webkit-background-origin: padding-box; 
                -webkit-background-clip: content-box;
                
                background:-moz-linear-gradient(
                    center bottom,
                    rgba(235, 235, 235, 1) 15%,
                    rgba(255, 255, 255,1) 60%
                
                )  repeat scroll 0 0 transparent;
            }
            
            .bar-preferences .header SPAN {
                background-image    : url(images/preferences.png);
                background-position : 0 0;
                background-repeat   : no-repeat;
                width:21px;
                height:21px;
                position:absolute;
                top:11px;
                left:10px;
            }
            
            .bar-preferences .header DIV {
                padding : 12px 0 0 39px;
            }
            
            .bar-preferences .cbcontainer {
                margin : 5px 0 0 5px;
            }
            
            .bar-preferences .first {
                margin-top : 10px;
            }
            
            .bar-preferences .underlined {
                border-bottom : 1px dashed #b9b9b9;
                margin        : 5px 0 0 5px;
                
                -moz-box-shadow    : 1px 1px 1px -1px rgba(255, 255, 255, 0.5);
                -webkit-box-shadow : 1px 1px 1px -1px rgba(255, 255, 255, 0.5);
                box-shadow         : 1px 1px 1px -1px rgba(255, 255, 255, 0.5);
            }
        ]]></a:style>
    
        <a:presentation>
            <a:main container=".">
                <div class="bar-preferences">
                    
                </div>
            </a:main>
        </a:presentation>
    </a:bar>
</a:skin>