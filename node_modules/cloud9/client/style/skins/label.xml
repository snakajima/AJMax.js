<a:skin xmlns:a="http://ajax.org/2005/aml"><a:label name="label">
    <a:style><![CDATA[
        .label{
            font-size: 8pt;
            font-family: Tahoma, Arial;
            overflow: hidden;
            cursor: default;
            color : #333;
            padding : 2px;
            white-space : nowrap;
        }

        .labelDisabled{
            color: #bebebe;
        }

        .tiny {
            font-size : 9px;
        }

        .error .label{
            background : url(images/alert.png) no-repeat 0 0;
            min-height : 37px;
            padding : 3px 0 0 45px;
        }
    ]]></a:style>

    <a:presentation>
        <a:main caption="." for="@for" minheight="17">
            <div class="label"> </div>
        </a:main>
    </a:presentation>
</a:label>
</a:skin>