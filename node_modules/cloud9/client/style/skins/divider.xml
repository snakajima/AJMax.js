<a:skin xmlns:a="http://ajax.org/2005/aml"><a:divider name="divider">
    <a:style><![CDATA[
        .divider{
            height : 0;
            border: 1px solid white;
            border-width : 1px 0 1px 0;
            border-color : #c3c3c3 white white #c3c3c3;
            line-height : 0;
            cursor : default;
        }

        .hbox .divider, .vdivider{
            width : 0;
            margin : 0 10px 0 10px;
            height: 17px;
            border-width : 0 1px 0 1px;
            height: 17px;
        }

        .divider span{
            color : #333;
            background : #f5f5f5;
            padding : 0 2px 0 2px;
            position : relative;
            top : -1px;
            display : none;
            font-size : 8pt;
        }

        .table .divider, .vbox .divider, .hbox .divider{
            margin : 0;
        }
    ]]></a:style>

    <a:presentation>
        <a:main container="." caption="span" minheight="2" minwidth="2">
            <div class="divider"><span> </span> </div>
        </a:main>
    </a:presentation>
</a:divider>
<a:divider name="divider-debugpanel">
    <a:style><![CDATA[
        .divider-debugpanel {
            position      : relative;
            overflow      : hidden;
            border-top    : 1px dotted #bebebe;
            border-bottom : 1px dotted #f3f3f3;
            height        : 4px;
        }
        
        .divider-debugpanel DIV {
            border-bottom : 1px dotted #bebebe;
            border-top    : 1px dotted #f3f3f3;
        }
    ]]></a:style>
    <a:style condition="apf.isGecko">
        .divider-debugpanel DIV {
            position : relative;
            left : -1px;
        }
    </a:style>

    <a:presentation>
        <a:main container="div" caption="div">
            <div class="divider-debugpanel">
                <div/>
            </div>
        </a:main>
    </a:presentation>
</a:divider>
    <a:divider name="c9-divider">
    <a:style><![CDATA[
        .c9-divider {
            position   : relative;
            overflow   : hidden;
            width:0px;
            height     : 23px;
            border-left:1px solid #000000;
            border-right:1px solid #404040;
        }
    ]]></a:style>
    
    <a:presentation>
        <a:main>
            <div class="c9-divider"> </div>
        </a:main>
    </a:presentation>
</a:divider>

<a:divider name="c9-divider-double">
    <a:style><![CDATA[
        .c9-divider-double {
            position   : relative;
            overflow   : hidden;
            width      : 2px;
            height     : 23px;
            border-left:1px solid #000000;
            border-right:1px solid #404040;
        }
        
        .c9-divider-double DIV {
            position:relative;
            width:1px;
            height:23px;
            border-left:1px solid #404040;
            border-right:1px solid #000000;
        }
    ]]></a:style>
    
    <a:presentation>
        <a:main>
            <div class="c9-divider-double">
                <div></div>
            </div>
        </a:main>
    </a:presentation>
</a:divider>
</a:skin>