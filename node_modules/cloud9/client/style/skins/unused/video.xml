<a:skin xmlns:a="http://ajax.org/2005/aml"><a:video name="video">
    <a:style><![CDATA[
        .video {
            line-height:300px;
            margin:0;
            padding:0;
            text-align:center;
            vertical-align:middle;
            overflow : hidden;
            background : #FAFBFC;
            color : white;
            font-size : 8pt;
            font-family : Tahoma, Arial;
        }

        .video #qt_event_source{
            position : absolute;
            left : 0;
            top : 0;
        }
    ]]></a:style>

    <a:presentation>
        <a:main container=".">
            <div class="video"> </div>
        </a:main>
    </a:presentation>
</a:video></a:skin>