    <a:style><![CDATA[
        .image{
            margin-bottom: 10px;
        }

        .imageDisabled{
        }

    ]]></a:style>

    <a:presentation>
        <a:main image="@src">
            <img src=""/>
        </a:main>
    </a:presentation>
</a:picture></a:skin>