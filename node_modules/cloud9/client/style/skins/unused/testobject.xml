<a:skin xmlns:a="http://ajax.org/2005/aml"><a:testobject name="testobject">
    <a:alias>aliasofobject</a:alias>

    <a:style><![CDATA[
        .testobject{
            border: 1px solid red;
            padding: 0 0 5px 0;
            overflow: hidden;
            height: 1%;
        }

        .testobject div{
            border: 1px solid red;
            width: 50px;
            height: 50px;
            margin: 5px;
            float: left;
            background: no-repeat 5px 5px;
        }

        .testobjectFocus{
            border: 2px solid black;
        }

        .testobjectDisabled{
            background-color: black;
        }
    ]]></a:style>
    <a:presentation>
        <a:main content=".">
            <div class="testobject">

            </div>
        </a:main>
        <a:item caption="." icon=".">
            <div> </div>
        </a:item>
    </a:presentation>
</a:testobject></a:skin>