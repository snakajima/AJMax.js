<a:skin xmlns:a="http://ajax.org/2005/aml"><a:pages name="pages">
    <a:style><![CDATA[
        .pages{
            overflow : hidden;
            position : relative;
        }

        .pages .page{
            display: none;

            position : absolute;
            left : 0;
            top : 0;
            right : 0;
            bottom : 0;
        }

        .pages .curpage{
            display: block;
        }
    ]]></a:style>

    <a:presentation>
        <a:main pages=".">
            <div class="pages">
            </div>
        </a:main>
        <a:page container=".">
            <div class="page"/>
        </a:page>
    </a:presentation>
</a:pages></a:skin>